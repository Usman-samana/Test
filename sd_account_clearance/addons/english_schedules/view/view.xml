                        <field name="schedule_a_eng" readonly="1" widget="html"/>
                        <separator string="Schedule B"/>
                        <field name="schedule_b_eng" readonly="1" widget="html"/>
                        <separator string="Schedule C"/>
                        <field name="schedule_c_eng" readonly="1" widget="html"/>

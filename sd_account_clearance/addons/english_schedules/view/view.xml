                        <separator string="Schedule F"/>
                        <field name="schedule_f_eng" readonly="1" widget="html"/>
                        <separator string="Schedule G"/>
                        <field name="schedule_g_eng" readonly="1" widget="html"/>
                        <separator string="Schedule H"/>
                        <field name="schedule_h_eng" readonly="1" widget="html"/>
                        <separator string="Schedule I"/>
                        <field name="schedule_i_eng" readonly="1" widget="html"/>
                    </page>
                </xpath>
            </field>
        </record>



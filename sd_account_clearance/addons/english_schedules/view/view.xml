        <record id="view_order_form_inherit_booking_schedule4" model="ir.ui.view">
            <field name="name">sale.order.form.inherit.schedule4</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="crm_extension.view_order_form_inherit_booking"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='spa_schedules']" position="after">
                    <page name="spa_schedules_eng" string="SPA Schedules English" readonly="1">
